[{
  "namespace": "quickmove",
  "types": [
    {
      "id": "MailIdentity",
      "type": "object",
      "properties": {
        "accountId": { "type": "string" },
        "composeHtml": { "type": "boolean" },
        "email": { "type": "string" },
        "id": { "type": "string" },
        "label": { "type": "string" },
        "name": { "type": "string" },
        "organization": { "type": "string", "optional": true },
        "replyTo": { "type": "string", "optional": true },
        "signature": { "type": "string", "optional": true },
        "signatureIsPlainText": { "type": "boolean", "optional": true }
      }
    }, {
      "id": "MailAccount",
      "type": "object",
      "properties": {
        "folders": { "type": "array", "items": { "$ref": "folders.MailFolder" }},
        "identities": { "type": "array", "items": { "$ref": "MailIdentity" }},
        "id": { "type": "string" },
        "name": { "type": "string" },
        "type": { "type": "string" }
      }
    }
  ],
  "functions": [
    {
      "name": "query",
      "type": "function",
      "async": true,
      "parameters": [
        {
          "name": "options",
          "type": "object",
          "properties": {
            "canFileMessages": { "type": "boolean", "optional": true },
            "recent": { "type": "boolean", "optional": true },
            "limit": { "type": "integer", "optional": true }
          }
        }
      ]
    },{
      "name": "setupLegacyShortcuts",
      "type": "function",
      "async": true,
      "parameters": [{
        "name": "enabled",
        "type": "boolean"
      }]
    },{
      "name": "focusThreadPane",
      "type": "function",
      "async": true,
      "parameters": []
    },{
      "name": "setLastMoveCopyFolder",
      "type": "function",
      "async": true,
      "parameters": [{
        "name": "destination",
        "$ref": "folders.MailFolder",
        "description": "The folder for the last move/copy operation"
      }, {
        "name": "isMove",
        "type": "boolean"
      }]
    }
  ]
}]
